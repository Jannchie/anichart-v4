{
  "compilerOptions": {
    "target": "ES2018",
    "jsx": "react-jsx",
    "lib": ["es2015"],
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
